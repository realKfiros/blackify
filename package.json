{
  "name": "blackify",
  "version": "0.1.3",
  "description": "compress files into archives and insert the archives to qr code",
  "main": "server.js",
  "bin": "lib/index.js",
  "files": [
    "lib/index.js"
  ],
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "compress": "node lib/index.js",
    "decompress-client": "concurrently \"npm run server\" \"cd client && npm run serve\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realKfiros/blackify.git"
  },
  "author": "realKfiros",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/realKfiros/blackify/issues"
  },
  "homepage": "https://github.com/realKfiros/blackify#readme",
  "dependencies": {
    "archiver": "^5.3.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "inquirer": "^8.1.2",
    "prettier": "^2.3.2",
    "qrcode": "^1.4.4",
    "snappy": "^6.3.5"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "husky": "^7.0.0",
    "nodemon": "^2.0.12"
  }
}
